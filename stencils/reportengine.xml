<shapes name="mxgraph.services"> <!-- Or mxgraph.compute, mxgraph.general -->

<shape name="reportEngine" aspect="fixed" w="190" h="190" strokewidth="1">
    <connections/>
    <foreground>
        <!-- 1. Background Rectangle - SCALED Consistent Style -->
        <save/>
        <strokecolor color="#85C1E9"/>
        <fillcolor color="#D6EAF8"/>
        <strokewidth width="5"/>
        <roundrect x="0" y="0" w="190" h="190" arcsize="13"/>
        <fillstroke/>
        <restore/>

        <!-- 2. Processing Symbol (Gear) - Represents the 'Engine' -->
        <save/>
        <strokecolor color="#AF601A"/> <!-- Brownish-Orange Border -->
        <fillcolor color="#F5B041"/>   <!-- Lighter Orange Fill -->
        <strokewidth width="2.5"/>
        <!-- Positioned slightly lower-left -->
        <!-- Gear Center approx: x=75, y=115 -->
        <ellipse x="50" y="90" w="50" h="50"/>
        <fillstroke/>
        <!-- Inner Cutout -->
        <ellipse x="67.5" y="107.5" w="15" h="15">
             <fillcolor color="#D6EAF8"/> <strokecolor color="#AF601A"/> <strokewidth width="1"/>
        </ellipse>
        <fillstroke/>
        <!-- Teeth (Simplified) -->
        <strokewidth width="4"/>
        <path> <move x="75" y="90"/> <line x="75" y="83"/> </path> <stroke/> <!-- Top -->
        <path> <move x="100" y="115"/> <line x="107" y="115"/> </path> <stroke/> <!-- Right -->
        <path> <move x="75" y="140"/> <line x="75" y="147"/> </path> <stroke/> <!-- Bottom -->
        <path> <move x="50" y="115"/> <line x="43" y="115"/> </path> <stroke/> <!-- Left -->
        <restore/> <!-- Restore state after gear -->

        <!-- 3. Report Document Symbol - Represents the 'Report' Output -->
        <save/>
        <strokecolor color="#424949"/> <!-- Dark Grey -->
        <fillcolor color="#FFFFFF"/>   <!-- White Paper -->
        <strokewidth width="2"/>
        <!-- Positioned slightly upper-right, overlapping gear -->
        <!-- Main page rectangle -->
        <rect x="90" y="40" w="70" h="90"/>
        <fillstroke/>
        <!-- Folded corner -->
        <path> <move x="135" y="40"/> <line x="160" y="65"/> <line x="135" y="65"/> <close/> </path>
        <fillstroke/>
        <path> <move x="135" y="40"/> <line x="135" y="65"/> </path> <stroke/>
        <!-- Simplified Chart/Graph Lines inside document -->
        <strokecolor color="#3498DB"/> <!-- Blue for chart lines -->
        <strokewidth width="3"/>
        <!-- Simple Bar Chart Example -->
        <rect x="100" y="90" w="10" h="30"/> <fillstroke/> <!-- Bar 1 -->
        <rect x="115" y="75" w="10" h="45"/> <fillstroke/> <!-- Bar 2 -->
        <rect x="130" y="100" w="10" h="20"/> <fillstroke/> <!-- Bar 3 -->
        <restore/> <!-- Restore state after document -->

    </foreground>
</shape>

</shapes>
