<shapes name="mxgraph.general"> <!-- Or mxgraph.storage, mxgraph.compute -->

<shape name="library" aspect="fixed" w="190" h="190" strokewidth="1">
    <connections/>
    <foreground>
        <!-- 1. Background Rectangle - SCALED Consistent Style -->
        <save/>
        <strokecolor color="#85C1E9"/>
        <fillcolor color="#D6EAF8"/>
        <strokewidth width="5"/>
        <roundrect x="0" y="0" w="190" h="190" arcsize="13"/>
        <fillstroke/>
        <restore/>

        <!-- 2. Bookshelf Symbol -->
        <save/>
        <!-- Bookshelf Frame -->
        <strokecolor color="#8B4513"/> <!-- SaddleBrown (Wood Color) -->
        <fillcolor color="#D2B48C"/>   <!-- Tan (Lighter Wood) -->
        <strokewidth width="4"/>
        <!-- Centered, slightly inset -->
        <rect x="35" y="35" w="120" h="120"/>
        <fillstroke/>

        <!-- Shelves -->
        <strokecolor color="#8B4513"/> <!-- Match frame -->
        <strokewidth width="3"/>
        <path> <move x="35" y="75"/> <line x="155" y="75"/> </path> <stroke/> <!-- Middle Shelf -->
        <path> <move x="35" y="115"/> <line x="155" y="115"/> </path> <stroke/> <!-- Lower Shelf -->
        <restore/>

        <!-- 3. Books on Shelves -->
        <save/>
        <strokecolor color="#424949"/> <!-- Dark grey book outline -->
        <strokewidth width="1.5"/>

        <!-- Top Shelf Books -->
        <fillcolor color="#27AE60"/> <!-- Green -->
        <rect x="45" y="40" w="25" h="35"/> <fillstroke/>
        <fillcolor color="#2980B9"/> <!-- Blue -->
        <rect x="75" y="40" w="25" h="35"/> <fillstroke/>
        <fillcolor color="#C0392B"/> <!-- Red -->
        <rect x="105" y="40" w="25" h="35"/> <fillstroke/>

        <!-- Middle Shelf Books -->
        <fillcolor color="#E67E22"/> <!-- Orange -->
        <rect x="45" y="80" w="25" h="35"/> <fillstroke/>
        <fillcolor color="#8E44AD"/> <!-- Purple -->
        <rect x="75" y="80" w="25" h="35"/> <fillstroke/>
        <fillcolor color="#F1C40F"/> <!-- Yellow -->
        <rect x="105" y="80" w="25" h="35"/> <fillstroke/>
        <fillcolor color="#1ABC9C"/> <!-- Turquoise -->
        <rect x="135" y="80" w="15" h="35"/> <fillstroke/> <!-- Slightly Thinner -->

         <!-- Bottom Shelf Books -->
        <fillcolor color="#34495E"/> <!-- Dark Grey/Blue -->
        <rect x="45" y="120" w="25" h="35"/> <fillstroke/>
        <fillcolor color="#7F8C8D"/> <!-- Medium Grey -->
        <rect x="75" y="120" w="25" h="35"/> <fillstroke/>

        <restore/> <!-- Restore state after books -->

    </foreground>
</shape>

</shapes>
