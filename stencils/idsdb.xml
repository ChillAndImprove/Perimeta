<shapes name="mxgraph.security"> <!-- Or mxgraph.storage, mxgraph.database -->

<shape name="identityStoreDatabase" aspect="fixed" w="190" h="190" strokewidth="1">
    <connections/>
    <foreground>
        <!-- 1. Background Rectangle - SCALED Consistent Style -->
        <save/>
        <strokecolor color="#85C1E9"/>
        <fillcolor color="#D6EAF8"/>
        <strokewidth width="5"/>
        <roundrect x="0" y="0" w="190" h="190" arcsize="13"/>
        <fillstroke/>
        <restore/>

        <!-- 2. Identity Symbol (Person Icon) -->
        <save/>
        <strokecolor color="#000000"/> <!-- Black Stroke -->
        <fillcolor color="#FFFFFF"/>   <!-- White Fill -->
        <strokewidth width="2.5"/>
        <!-- Positioned Left -->
        <!-- Head -->
        <ellipse x="25" y="65" w="40" h="40"/> <!-- Shifted Left -->
        <fillstroke/>
        <!-- Body -->
        <roundrect x="15" y="107" w="60" h="43" arcsize="13"/> <!-- Shifted Left -->
        <fillstroke/>
        <restore/>

        <!-- 3. Database Cylinder Symbol -->
        <save/>
        <strokecolor color="#424949"/> <!-- Dark Grey Outline -->
        <fillcolor color="#ABB2B9"/>   <!-- Medium Grey Fill -->
        <strokewidth width="3"/>
        <!-- Positioned Right -->
        <!-- Top Ellipse -->
        <ellipse x="95" y="45" w="90" h="20"/> <!-- Shifted Right -->
        <fillstroke/>
        <!-- Middle Ellipse -->
        <ellipse x="95" y="85" w="90" h="20"/> <!-- Shifted Right -->
        <fillstroke/>
        <!-- Bottom Ellipse -->
        <ellipse x="95" y="125" w="90" h="20"/> <!-- Shifted Right -->
        <fillstroke/>
        <!-- Vertical Side Lines -->
        <strokecolor color="#424949"/> <strokewidth width="3"/>
        <!-- Left Side -->
        <path> <move x="95" y="55"/> <line x="95" y="135"/> </path> <!-- Shifted Right -->
        <stroke/>
        <!-- Right Side -->
        <path> <move x="185" y="55"/> <line x="185" y="135"/> </path> <!-- Shifted Right -->
        <stroke/>
        <restore/>

    </foreground>
</shape>

</shapes>
