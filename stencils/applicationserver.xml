<shapes name="mxgraph.servers"> <!-- Or mxgraph.compute, mxgraph.general -->

<shape name="applicationServer" aspect="fixed" w="190" h="190" strokewidth="1">
    <connections/>
    <foreground>
        <!-- 1. Background Rectangle - SCALED Consistent Style -->
        <save/>
        <strokecolor color="#85C1E9"/>
        <fillcolor color="#D6EAF8"/>
        <strokewidth width="5"/>
        <roundrect x="0" y="0" w="190" h="190" arcsize="13"/>
        <fillstroke/>
        <restore/>

        <!-- 2. Base Server Box - SCALED -->
        <save/>
        <strokecolor color="#424949"/>
        <fillcolor color="#ABB2B9"/>
        <strokewidth width="3"/>
        <!-- Centered vertical rectangle -->
        <rect x="40" y="32" w="111" h="127"/>
        <fillstroke/>
        <restore/>

        <!-- 3. Processing Symbol (Gear) - Representing Application Logic -->
        <save/>
        <strokecolor color="#AF601A"/> <!-- Brownish-Orange Border -->
        <fillcolor color="#F5B041"/>   <!-- Lighter Orange Fill -->
        <strokewidth width="2.5"/>
        <!-- Positioned centrally within the server box -->
        <!-- Gear Center approx: x=95.5, y=95.5 -->
        <ellipse x="75.5" y="75.5" w="40" h="40"/> <!-- Slightly smaller gear -->
        <fillstroke/>
        <!-- Inner Cutout -->
        <ellipse x="88" y="88" w="15" h="15"> <!-- Adjusted center -->
             <fillcolor color="#ABB2B9"/> <!-- Match server box fill -->
             <strokecolor color="#AF601A"/> <strokewidth width="1"/>
        </ellipse>
        <fillstroke/>
        <!-- Teeth (Simplified) -->
        <strokewidth width="3.5"/>
        <path> <move x="95.5" y="75.5"/> <line x="95.5" y="69.5"/> </path> <stroke/> <!-- Top -->
        <path> <move x="115.5" y="95.5"/> <line x="121.5" y="95.5"/> </path> <stroke/> <!-- Right -->
        <path> <move x="95.5" y="115.5"/> <line x="95.5" y="121.5"/> </path> <stroke/> <!-- Bottom -->
        <path> <move x="75.5" y="95.5"/> <line x="69.5" y="95.5"/> </path> <stroke/> <!-- Left -->
        <!-- Diagonals (Simplified) -->
        <path> <move x="109.5" y="81.5"/> <line x="113.5" y="77.5"/> </path> <stroke/>
        <path> <move x="109.5" y="109.5"/> <line x="113.5" y="113.5"/> </path> <stroke/>
        <path> <move x="81.5" y="109.5"/> <line x="77.5" y="113.5"/> </path> <stroke/>
        <path> <move x="81.5" y="81.5"/> <line x="77.5" y="77.5"/> </path> <stroke/>
        <restore/> <!-- Restore state after gear -->

    </foreground>
</shape>

</shapes>
